{
  "env" : {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-duplicate-imports": 0,
    "comma-spacing": [1, {"before": false, "after": true}],
    "semi": [1, "always"],
    "semi-spacing": [1, {"before": false, "after": true}],
    "quotes": [2, "double"],
    "strict": 0,
    "no-var": 0,
    "indent": [2, 2, {"SwitchCase": 1}]
  }
}